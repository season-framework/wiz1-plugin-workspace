ejoin=\"round\")\n                        path(stroke=\"none\" d=\"M0 0h24v24H0z\")\n                        circle(cx=\"10\" cy=\"10\" r=\"7\")\n                        line(x1=\"21\" y1=\"21\" x2=\"15\" y2=\"15\")\n\n            .root-tree.mb-1(ng-repeat=\"item in browse.data\" class=\"{{item.package.id == app.id ? 'active' : ''}}\" ng-click=\"browse.select(item)\" ng-if=\"!item.hide\")\n                .root-title\n                    table\n                        tr \n                            td.w-1(style=\"vertical-align: top;\")\n                                i.fas.fa-code.mr-2\n                            td(style=\"line-height: 1;\")\n                                span {{item.package.title}}\n                                br\n                                span.namespace {{item.package.route}}\n\n            a.root-tree.mb-1(href=\"/wiz/admin/workspace/routes/editor/new\")\n                .root-title.text-center.p-1.text-white\n                    i.fas.fa-plus.mr-2\n                    span Create New Route\n\n        div(ng-if=\"extdata.browse_active=='app'\")\n            .input-icon.mb-3\n                input.form-control#search(type=\"text\" placeholder=\"Search\" style=\"color: #545d6d !important;\" ng-model=\"extdata.searchvalue\" ng-change=\"extdata.browse_search(extdata.searchvalue)\" autocomplete='off')\n                span.input-icon-addon\n                    svg.icon(xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" stroke-width=\"2\" stroke=\"currentColor\" fill=\"none\" stroke-linecap=\"round\" stroke-linejoin=\"round\")\n                        path(stroke=\"none\" d=\"M0 0h24v24H0z\")\n                        circle(cx=\"10\" cy=\"10\" r=\"7\")\n                        line(x1=\"21\" y1=\"21\" x2=\"15\" y2=\"15\")\n                    \n            .root-tree.mb-1(ng-repeat=\"item in extdata.apps\" ng-if=\"!item.hide\" ng-click=\"extdata.browse_select(item)\")\n                .root-title\n  